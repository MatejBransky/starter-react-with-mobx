{
  "name": "starter-react-with-mobx",
  "version": "1.0.0",
  "repository": "git@github.com:MatejBransky/starter-react-with-mobx.git",
  "author": "Matej Bransky <matej.bransky@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "parcel src/index.html",
    "build": "rimraf dist && parcel build src/index.html",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore . --ext ts --ext tsx --ext js --ext jsx",
    "lint:fix": "npm run lint --fix",
    "format": "prettier --write src/**/*.{js,ts,tsx}"
  },
  "devDependencies": {
    "@types/react": "^16.8.6",
    "@types/react-dom": "^16.8.2",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "eslint": "^5.15.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "parcel-bundler": "^1.11.0",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "parcel-plugin-static-files-copy": "^2.0.0",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "sass": "^1.17.2",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended": "^2.1.0",
    "typescript": "^3.3.3333"
  },
  "dependencies": {
    "mobx": "^5.9.0",
    "mobx-react-lite": "^1.1.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3"
  }
}
