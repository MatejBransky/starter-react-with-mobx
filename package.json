{
  "name": "starter-react-with-mobx",
  "version": "1.0.0",
  "repository": "git@github.com:MatejBransky/starter-react-with-mobx.git",
  "author": "Matej Bransky <matej.bransky@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "parcel src/index.html",
    "build": "rimraf dist && parcel build src/index.html",
    "type-check": "tsc --skipLibCheck",
    "lint": "eslint --ext ts --ext tsx --ext js --ext jsx",
    "lint:fix": "npm run lint --fix",
    "format": "prettier --write {src,tests,tests-e2e}/**/*.{js,ts,tsx}",
    "test": "jest",
    "tdd": "jest --watch",
    "test:e2e": "cypress open"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^4.0.3",
    "@types/jest": "^24.0.11",
    "@types/react": "^16.8.10",
    "@types/react-dom": "^16.8.3",
    "@typescript-eslint/eslint-plugin": "^1.5.0",
    "@typescript-eslint/parser": "^1.5.0",
    "cypress": "^3.2.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.6.0",
    "husky": "^1.3.1",
    "jest": "^24.5.0",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^8.1.5",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "parcel-plugin-static-files-copy": "^2.0.0",
    "prettier": "^1.16.4",
    "react-testing-library": "^6.0.3",
    "rimraf": "^2.6.3",
    "sass": "^1.17.3",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended": "^2.1.0",
    "ts-jest": "^24.0.1",
    "ts-loader": "^5.3.3",
    "typescript": "^3.4.1",
    "webpack": "^4.29.6"
  },
  "dependencies": {
    "mobx": "^5.9.4",
    "mobx-react-lite": "^1.2.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
